          "settings": {
            "custom_liquid": "<div class=\"rte-table-wrapper\">\n  <table class=\"comparison-matrix-table\">\n    <thead>\n      <tr class=\"comparison-matrix-header\">\n        <th class=\"comparison-matrix-cell comparison-matrix-cell--header\">\n          <a href=\"#\" class=\"comparison-matrix-product-link\">\n            <img\n              src=\"https://via.placeholder.com/80x80\"\n              alt=\"Product 1\"\n              width=\"80\"\n              height=\"80\"\n              class=\"comparison-matrix-image\"\n            >\n          </a>\n          <a href=\"#\" class=\"comparison-matrix-title\">\n            Undaria Algae™ Body Oil\n          </a>\n        </th>\n        <th class=\"comparison-matrix-cell comparison-matrix-cell--header\">\n          <a href=\"#\" class=\"comparison-matrix-product-link\">\n            <img\n              src=\"https://via.placeholder.com/80x80\"\n              alt=\"Product 2\"\n              width=\"80\"\n              height=\"80\"\n              class=\"comparison-matrix-image\"\n            >\n          </a>\n          <a href=\"#\" class=\"comparison-matrix-title\">\n            Undaria Algae™ Body Butter\n          </a>\n        </th>\n        <th class=\"comparison-matrix-cell comparison-matrix-cell--header\">\n          <a href=\"#\" class=\"comparison-matrix-product-link\">\n            <img\n              src=\"https://via.placeholder.com/80x80\"\n              alt=\"Product 3\"\n              width=\"80\"\n              height=\"80\"\n              class=\"comparison-matrix-image\"\n            >\n          </a>\n          <a href=\"#\" class=\"comparison-matrix-title\">\n            Undaria Algae™ Body Lotion\n          </a>\n        </th>\n      </tr>\n    </thead>\n    <tbody>\n      <tr>\n        <th colspan=\"3\" class=\"comparison-matrix-section-header\">What It Is</th>\n      </tr>\n      <tr class=\"comparison-matrix-row\">\n        <td class=\"comparison-matrix-cell\">\n          <span>A nourishing body oil that leaves skin glowing and hydrated</span>\n        </td>\n        <td class=\"comparison-matrix-cell\">\n          <span>A rich body butter with anti-aging benefits and matte finish</span>\n        </td>\n        <td class=\"comparison-matrix-cell\">\n          <span>A silky body lotion with smoothing and hydrating properties</span>\n        </td>\n      </tr>\n\n      <tr>\n        <th colspan=\"3\" class=\"comparison-matrix-section-header\">Who It's For</th>\n      </tr>\n      <tr class=\"comparison-matrix-row\">\n        <td class=\"comparison-matrix-cell\">\n          <span>All skin types, Dry skin</span>\n        </td>\n        <td class=\"comparison-matrix-cell\">\n          <span>Mature skin, Dry skin</span>\n        </td>\n        <td class=\"comparison-matrix-cell\">\n          <span>All skin types, Normal skin</span>\n        </td>\n      </tr>\n\n      <tr>\n        <th colspan=\"3\" class=\"comparison-matrix-section-header\">Price</th>\n      </tr>\n      <tr class=\"comparison-matrix-row\">\n        <td class=\"comparison-matrix-cell\">\n          $48.00\n        </td>\n        <td class=\"comparison-matrix-cell\">\n          $52.00\n        </td>\n        <td class=\"comparison-matrix-cell\">\n          $44.00\n        </td>\n      </tr>\n\n      <tr>\n        <th colspan=\"3\" class=\"comparison-matrix-section-header\">Finish</th>\n      </tr>\n      <tr class=\"comparison-matrix-row\">\n        <td class=\"comparison-matrix-cell\">\n          Glowing\n        </td>\n        <td class=\"comparison-matrix-cell\">\n          Matte\n        </td>\n        <td class=\"comparison-matrix-cell\">\n          Silky\n        </td>\n      </tr>\n\n      <tr>\n        <th colspan=\"3\" class=\"comparison-matrix-section-header\">Scent Profile</th>\n      </tr>\n      <tr class=\"comparison-matrix-row\">\n        <td class=\"comparison-matrix-cell\">\n          Citrus and cypress with refreshing marine notes\n        </td>\n        <td class=\"compari